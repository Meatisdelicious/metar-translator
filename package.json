{
  "name": "metar-taf-translator",
  "version": "1.0.0",
  "description": "// Notes :\r // dot.env permet de charger des valeurs depuis mon environnement\r // xo, gulp, mocha, test unitaires, git, gitub action\r // programmation fonctionnelle uniquement",
  "main": "metar-translator.js",
  "type": "module",
  "scripts": {
    "test": "mocha --recursive",
    "code-style": "xo",
    "gulp": "gulp"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.3.6",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "mocha": "^10.0.0",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "xo": "^0.48.0"
  },
  "xo": {
    "space": 2,
    "prettier": true,
    "rules": {
      "unicorn/no-fn-reference-in-iterator": 0,
      "unicorn/no-array-callback-reference": 0,
      "unicorn/no-array-method-this-argument": 0,
      "unicorn/no-array-reduce": 0
    },
    "envs": [
      "mocha"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
